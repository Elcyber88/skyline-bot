{
  "name": "skyline-bot",
  "version": "1.0.0",
  "description": "This is the bot for the official Skyline Emulator Discord Server",
  "main": "main.js",
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "start": "tsc && node app/main.js",
    "tsc": "tsc",
    "tscw": "tsc-watch --onSuccess \"node ./build/main.js\"",
    "fmt": "prettier --write \"**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PixelyIon/skyline-bot.git"
  },
  "author": "https://github.com/PixelyIon/skyline-bot/contributors",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/PixelyIon/skyline-bot/issues"
  },
  "homepage": "https://github.com/PixelyIon/skyline-bot#readme",
  "dependencies": {
    "@types/node-fetch": "^2.5.4",
    "discord.js": "^11.5.1",
    "node-fetch": "^2.6.0",
    "tsc": "^1.20150623.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.14",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.7.3"
  }
}
